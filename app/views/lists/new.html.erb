<h1>新規投稿</h1>

<!--form_with部分-->
<!--Listモデルの情報をもとに新しくオブジェクトを作成する-->
<!--List.new→＠listに修正-->
<!--どのURLへどのHTTPメソッドで送信するか厳密に指定-->
<%= form_with model: @list, url: '/lists', method: :post do |f| %>
  <!--タイトル-->
  <h4>タイトル</h4>
  <%= f.text_field :title %>

  <!--本文-->
  <h4>本文</h4>
  <%= f.text_area :body %>

  <!--投稿ボタン-->
  <%= f.submit '投稿' %>


<% end %>